<div class="item_table">
    <strong>{{ result.NAME ~ ':' }}</strong>
    <div class="wrap_table">
        <table>
            <thead>
            <tr>
                <td><b>Название услуги</b></td>
                {% for prop in result.ITEMS[0].PROPERTIES %}
                    <td><b>{{ prop.NAME }}</b></td>
                {% endfor %}
            </tr>
            </thead>
            <tbody>
            {% for item in result.ITEMS %}
                <tr id="{{ add_editing_actions(item, template) }}">
                    <td>{{ item.NAME }}</td>
                    {# TODO pick column properties only #}
                    {% for prop in item.PROPERTIES %}
                        <td>{{ prop.VALUE }}</td>
                    {% endfor %}
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
</div>
